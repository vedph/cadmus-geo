<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <IncludeSymbols>true</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <Authors>Daniele Fusi</Authors>
	  <Company>Fusi</Company>
	  <Product>Cadmus</Product>
	  <Description>Cadmus generic geography seeder components.</Description>
	  <NeutralLanguage>en-US</NeutralLanguage>
	  <Version>2.0.1</Version>
	  <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
	  <Copyright>by Daniele Fusi</Copyright>
	  <PackageTags>Cadmus;geography</PackageTags>
	  <FileVersion>2.0.1</FileVersion>
	  <AssemblyVersion>2.0.1</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Seed" Version="6.1.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Cadmus.Geo.Parts\Cadmus.Geo.Parts.csproj" />
  </ItemGroup>

</Project>
